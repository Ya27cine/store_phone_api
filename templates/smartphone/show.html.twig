{% extends 'base.html.twig' %}

{% block title %}Smartphone
{% endblock %}

{% block body %}

	<div class="container my-5">
		<h1>Smartphone</h1>

		<div class="row">
			<table class="table">
				<tbody>
					<tr>
						<th>Id</th>
						<td>{{ smartphone.id }}</td>
					</tr>
					<tr>
						<th>Marque</th>
						<td>{{ smartphone.Marque }}</td>
					</tr>
					<tr>
						<th>Model</th>
						<td>{{ smartphone.Model }}</td>
					</tr>
					<tr>
						<th>Name</th>
						<td>{{ smartphone.Name }}</td>
					</tr>
					<tr>
						<th>Description</th>
						<td>{{ smartphone.description }}</td>
					</tr>
				</tbody>
			</table>
		</div>
	</div>


	<a href="{{ path('smartphone_index') }}">back to list</a>

	<a href="{{ path('smartphone_edit', {'id': smartphone.id}) }}">edit</a>

	{{ include('smartphone/_delete_form.html.twig') }}
